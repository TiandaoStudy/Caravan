<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <extensions>
    <add assembly="Finsa.Caravan.DataAccess" />
  </extensions>
  <targets>
    <target name="caravan" xsi:type="CaravanLog" />
  </targets>
  <rules>
    <logger name="*" minlevel="Trace" writeTo="caravan" />
  </rules>
</nlog>