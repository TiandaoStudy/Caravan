<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="caravan.common" requirePermission="false" type="System.Configuration.NameValueSectionHandler, System" />
    <section name="caravan.dataAccess" requirePermission="false" type="System.Configuration.NameValueSectionHandler, System" />
    <section name="caravan.webService" requirePermission="false" type="System.Configuration.NameValueSectionHandler,System,Version=1.0.3300.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
  </configSections>
  <caravan.common>
    <add key="AppName" value="wscaravan" />
    <add key="AppDescription" value="wsCaravan" />
    <add key="Logging_CaravanVariablesContext_Interval" value="00:30:00" />
    <add key="CacheLifetime" value="00:30:00" />
  </caravan.common>
  <caravan.dataAccess>
    <add key="ConnectionString" value="Data Source=ORASCONTRINO;User Id=CARAVAN;Password=caravan;" />
    <add key="DataSourceKind" value="Oracle" />
    <add key="SqlSchema" value="CARAVAN" />
    <!--<add key="ConnectionString" value="Data Source=.\SQLEXPRESS;Initial Catalog=caravan;User ID=caravan;Password=caravan" />
    <add key="DataSourceKind" value="SqlServer" />
    <add key="SqlSchema" value="dbo" />-->
    <add key="SqlInitializer" value="CreateDatabaseIfNotExists" />
    <add key="OracleStatementCacheSize" value="10" />
    <add key="RestTestAuthObject" value="_TEST_TOKEN_" />
    <add key="MongoDbName" value="" />
    <add key="Logging_SettingsCache_Interval" value="00:30:00" />
    <add key="Drivers_Sql_EFPregeneratedViews_Path" value="~/App_Data/EFPregeneratedViews" />
    <add key="Drivers_Sql_EFPregeneratedViews_CaravanViewsFileName" value="CaravanViews.xml" />
  </caravan.dataAccess>
  <caravan.webService>
    <add key="Identity_SigningCertificatePath" value="~/Caravan.pfx" />
    <add key="Identity_SigningCertificatePassword" value="FinsaPassword" />
    <add key="Security_EnableCaravanServices" value="False" />
  </caravan.webService>
