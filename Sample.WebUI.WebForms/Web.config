<?xml version="1.0" encoding="utf-8"?>

<configuration>
    <configSections>
        <!-- Caravan Configuration Headers (START) -->
        <section name="FlexWebConfiguration" type="FLEX.WebForms.Configuration, Finsa.Caravan.WebForms" />
        <!-- Caravan Configuration Headers (END) -->
        
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <!-- Caravan Configuration Headers (START) -->
            <section name="Finsa.Caravan.Common.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
            <section name="Finsa.Caravan.DataAccess.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
            <!-- Caravan Configuration Headers (END) -->
            
            <!-- KVLite Configuration Header (START) -->
            <section name="PommaLabs.KVLite.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
            <!-- KVLite Configuration Header (END) -->

            <!-- Armando Configuration Header (START) -->
            <section name="PommaLabs.Armando.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
            <!-- Armando Configuration Header (END) -->
        </sectionGroup>
        
        <!-- WebMarkupMin Configuration Section (START) -->
        <sectionGroup name="webMarkupMin">
            <section name="core" type="WebMarkupMin.Core.Configuration.CoreConfiguration, WebMarkupMin.Core" />
        </sectionGroup>
        <!-- WebMarkupMin Configuration Section (END) -->
        
        <!-- Oracle Configuration Section (START) -->
        <section name="oracle.manageddataaccess.client" type="OracleInternal.Common.ODPMSectionHandler, Oracle.ManagedDataAccess" />
        <!-- Oracle Configuration Section (START) -->
    </configSections>
    
    <!-- Caravan Configuration Sections (START) -->
    <FlexWebConfiguration AjaxLookupsXmlPath="~/App_Data/XmlSettings/AjaxLookups" CheckSecurity="false" ControlExtendersFolder="~/MyFLEX/ControlExtenders" DynamicReportsFolder="~/App_Data/XmlSettings/DynamicReports" ErrorManagerTypeInfo="FLEX.Sample.WebUI.MyFLEX.Managers.ErrorManager, FLEX.Sample.WebUI" LookupsXmlPath="~/App_Data/XmlSettings/Lookups" MenuBarBrandClick="window.location.href = 'http://www.finsa.it';" MenuBarHomeClick="FLEX_RunFromMenu('Home.aspx');" MenuBarInfoClick="return true;" MenuBarLogoutClick="return true;" MenuBarXmlPath="~/App_Data/XmlSettings/MenuBar.xml" PageManagerTypeInfo="FLEX.Sample.WebUI.MyFLEX.Managers.PageManager, FLEX.Sample.WebUI" SecurityManagerTypeInfo="FLEX.Sample.WebUI.MyFLEX.Managers.SecurityManager, FLEX.Sample.WebUI" SessionExpiredPageUrl="Expired.aspx" />
    <!-- Caravan Configuration Sections (END) -->
    
    <applicationSettings>
        <!-- Caravan Configuration Sections (START) -->
        <Finsa.Caravan.Common.Properties.Settings>
            <setting name="ApplicationName" serializeAs="String">
                <value>caravan.sample.webforms</value>
            </setting>
            <setting name="ApplicationDescription" serializeAs="String">
                <value>Caravan.Sample.WebForms</value>
            </setting>
        </Finsa.Caravan.Common.Properties.Settings>

        <Finsa.Caravan.DataAccess.Properties.Settings>
            <setting name="ConnectionString" serializeAs="String">
                <value>Data Source=RINA11;User Id=ascesi;password=ascesi;</value>
            </setting>
            <setting name="DataAccessKind" serializeAs="String">
                <value>Oracle</value>
            </setting>
            <setting name="OracleUser" serializeAs="String">
                <value>ASCESI</value>
            </setting>
            <setting name="PersistConnectionString" serializeAs="String">
                <value>True</value>
            </setting>
        </Finsa.Caravan.DataAccess.Properties.Settings>
        <!-- Caravan Configuration Sections (END) -->

        <!-- KVLite Configuration Section (START) -->
        <PommaLabs.KVLite.Properties.Settings>
            <setting name="PersistentCache_DefaultCacheFile" serializeAs="String">
                <value>~/App_Data/PersistentCache/PersistentCache.sqlite</value>
            </setting>
            <setting name="AllCaches_DefaultStaticIntervalInDays" serializeAs="String">
                <value>30</value>
            </setting>
            <setting name="PersistentCache_DefaultMaxCacheSizeInMB" serializeAs="String">
                <value>256</value>
            </setting>
            <setting name="PersistentCache_DefaultMaxJournalSizeInMB" serializeAs="String">
                <value>32</value>
            </setting>
            <setting name="PersistentCache_DefaultInsertionCountBeforeAutoClean" serializeAs="String">
                <value>100</value>
            </setting>
            <setting name="VolatileCache_DefaultMaxCacheSizeInMB" serializeAs="String">
                <value>128</value>
            </setting>
            <setting name="VolatileCache_DefaultMaxJournalSizeInMB" serializeAs="String">
                <value>32</value>
            </setting>
            <setting name="VolatileCache_DefaultInsertionCountBeforeAutoClean" serializeAs="String">
                <value>100</value>
            </setting>
        </PommaLabs.KVLite.Properties.Settings>
        <!-- KVLite Configuration Section (END) -->

        <!-- Armando Configuration Section (START) -->
        <PommaLabs.Armando.My.MySettings>
            <setting name="ScriptsLanguage" serializeAs="String">
                <value>C#</value>
            </setting>
            <setting name="ScriptsPath" serializeAs="String">
                <value>~/App_Data/DataScripts</value>
            </setting>
            <setting name="CompiledScriptsPath" serializeAs="String">
                <value>~/App_Data/CompiledScripts</value>
            </setting>
            <setting name="EnableScriptsDebug" serializeAs="String">
                <value>True</value>
            </setting>
        </PommaLabs.Armando.My.MySettings>
        <!-- Armando Configuration Section (END) -->
    </applicationSettings>
    
    <!-- Oracle Configuration Sections (START) -->
    <oracle.manageddataaccess.client>
        <version number="*">
            <settings>
                <setting name="TNS_ADMIN" value="C:\oracle\11g_32\network\admin" />
            </settings>
        </version>
    </oracle.manageddataaccess.client>
    <!-- Oracle Configuration Sections (END) -->
    
    <system.data>
        <DbProviderFactories>
            <remove invariant="Oracle.ManagedDataAccess.Client" />
            <add name="Oracle Data Provider for .NET" invariant="Oracle.ManagedDataAccess.Client" description=".Net Framework Data Provider for Oracle" type="Oracle.ManagedDataAccess.Client.OracleClientFactory, Oracle.ManagedDataAccess" />
            <remove invariant="System.Data.SqlServerCe.4.0" />
            <add name="Microsoft SQL Server Compact Data Provider 4.0" invariant="System.Data.SqlServerCe.4.0" description=".NET Framework Data Provider for Microsoft SQL Server Compact" type="System.Data.SqlServerCe.SqlCeProviderFactory, System.Data.SqlServerCe, Version=4.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" />
        </DbProviderFactories>
    </system.data>
    
    <!-- Start Paths for FLEX -->
    <location path="FLEX/Fonts">
        <system.web>
            <authorization>
                <allow users="*" />
            </authorization>
        </system.web>
    </location>
    <location path="FLEX/Images">
        <system.web>
            <authorization>
                <allow users="*" />
            </authorization>
        </system.web>
    </location>
    <location path="FLEX/Scripts">
        <system.web>
            <authorization>
                <allow users="*" />
            </authorization>
        </system.web>
    </location>
    <location path="FLEX/Styles">
        <system.web>
            <authorization>
                <allow users="*" />
            </authorization>
        </system.web>
    </location>
    <!-- End Paths for FLEX -->
    
    <connectionStrings>
        <add name="Northwind" connectionString="Data Source={0}; Persist Security Info=False;" providerName="System.Data.SqlServerCe.4.0" />
        <add name="ASCESI" connectionString="Data Source=RINA11;User Id=ascesi;password=ascesi;" providerName="Oracle.ManagedDataAccess.Client" />
    </connectionStrings>
    
    <system.web>
        <compilation defaultLanguage="cs" debug="true" targetFramework="4.5" />
        <customErrors mode="RemoteOnly" />
        <authentication mode="Forms">
            <!-- TIMEOUT Deve essere 1 minuto meno del timeout di sessione -->
            <forms timeout="19" />
        </authentication>
        <pages clientIDMode="AutoID" validateRequest="false" controlRenderingCompatibilityVersion="4.0" />
        <globalization requestEncoding="utf-8" responseEncoding="utf-8" culture="it-IT" />
        
        <!-- Abilito l'upload di file grandi al massimo 10 MB -->
        <!-- La clausola serve "requestValidationMode" per disabilitare la validazione del tutto -->
        <httpRuntime maxRequestLength="10240" requestValidationMode="2.0" />
        
        <!-- TIMEOUT Deve essere 1 minuto piu' del timeout di Autenticazione -->
        <sessionState mode="InProc" cookieless="false" timeout="20" />
    </system.web>
    
    <system.webServer>
        <handlers>
            <add name="ReportViewerWebControlHandler" preCondition="integratedMode" verb="*" path="Reserved.ReportViewerWebControl.axd" type="Microsoft.Reporting.WebForms.HttpHandler, Microsoft.ReportViewer.WebForms, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" />   
        </handlers>
        
        <!-- Required for HTML5 and CSS3 multimedia content (START) -->
        <staticContent>
            <remove fileExtension=".mp4" />
            <mimeMap fileExtension=".mp4" mimeType="video/mp4" />
            <remove fileExtension=".m4v" />
            <mimeMap fileExtension=".m4v" mimeType="video/m4v" />
            <remove fileExtension=".ogg" />
            <mimeMap fileExtension=".ogg" mimeType="video/ogg" />
            <remove fileExtension=".ogv" />
            <mimeMap fileExtension=".ogv" mimeType="video/ogg" />
            <remove fileExtension=".webm" />
            <mimeMap fileExtension=".webm" mimeType="video/webm" />
            <remove fileExtension=".oga" />
            <mimeMap fileExtension=".oga" mimeType="audio/ogg" />
            <remove fileExtension=".spx" />
            <mimeMap fileExtension=".spx" mimeType="audio/ogg" />
            <remove fileExtension=".svg" />
            <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
            <remove fileExtension=".svgz" />
            <mimeMap fileExtension=".svgz" mimeType="image/svg+xml" />
            <remove fileExtension=".eot" />
            <mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject" />
            <remove fileExtension=".otf" />
            <mimeMap fileExtension=".otf" mimeType="font/otf" />
            <remove fileExtension=".woff" />
            <mimeMap fileExtension=".woff" mimeType="font/x-woff" />
        </staticContent>
        <!-- Required for HTML5 and CSS3 multimedia content (END) -->
        
        <!-- IIS Content Compression (START) -->
        <httpCompression directory="%SystemDrive%\inetpub\temp\IIS Temporary Compressed Files">
            <scheme name="gzip" dll="%Windir%\system32\inetsrv\gzip.dll" />
            <dynamicTypes>
                <add mimeType="text/*" enabled="true" />
                <add mimeType="message/*" enabled="true" />
                <add mimeType="application/x-javascript" enabled="true" />
                <add mimeType="*/*" enabled="false" />
            </dynamicTypes>
            <staticTypes>
                <add mimeType="text/*" enabled="true" />
                <add mimeType="message/*" enabled="true" />
                <add mimeType="application/x-javascript" enabled="true" />
                <add mimeType="*/*" enabled="false" />
            </staticTypes>
        </httpCompression>
        <urlCompression doStaticCompression="true" doDynamicCompression="true" />
        <!-- IIS Content Compression (END) -->
    </system.webServer>
    
    <!-- WebMarkupMin Configuration Section (START) -->
    <webMarkupMin xmlns="http://tempuri.org/WebMarkupMin.Configuration.xsd">
        <core>
            <html whitespaceMinificationMode="Medium" removeHtmlComments="true" removeHtmlCommentsFromScriptsAndStyles="true" removeCdataSectionsFromScriptsAndStyles="true" useShortDoctype="false" useMetaCharsetTag="true" emptyTagRenderMode="NoSlash" removeOptionalEndTags="true" removeTagsWithoutContent="false" collapseBooleanAttributes="true" removeEmptyAttributes="true" attributeQuotesRemovalMode="Html5" removeRedundantAttributes="true" removeJsTypeAttributes="true" removeCssTypeAttributes="true" removeHttpProtocolFromAttributes="false" removeHttpsProtocolFromAttributes="false" removeJsProtocolFromAttributes="true" minifyEmbeddedCssCode="true" minifyInlineCssCode="true" minifyEmbeddedJsCode="false" minifyInlineJsCode="true" />
            <css>
                <minifiers>
                    <add name="NullCssMinifier" displayName="Null CSS Minifier" type="WebMarkupMin.Core.Minifiers.NullCssMinifier, WebMarkupMin.Core" />
                    <add name="KristensenCssMinifier" displayName="Mads Kristensen's CSS minifier" type="WebMarkupMin.Core.Minifiers.KristensenCssMinifier, WebMarkupMin.Core" />
                </minifiers>
            </css>
            <js>
                <minifiers>
                    <add name="NullJsMinifier" displayName="Null JS Minifier" type="WebMarkupMin.Core.Minifiers.NullJsMinifier, WebMarkupMin.Core" />
                    <add name="CrockfordJsMinifier" displayName="Douglas Crockford's JS Minifier" type="WebMarkupMin.Core.Minifiers.CrockfordJsMinifier, WebMarkupMin.Core" />
                </minifiers>
            </js>
            <logging>
                <loggers>
                    <add name="NullLogger" displayName="Null Logger" type="WebMarkupMin.Core.Loggers.NullLogger, WebMarkupMin.Core" />
                    <add name="ThrowExceptionLogger" displayName="Throw exception logger" type="WebMarkupMin.Core.Loggers.ThrowExceptionLogger, WebMarkupMin.Core" />
                </loggers>
            </logging>
        </core>
    </webMarkupMin>
    <!-- WebMarkupMin Configuration Section (END) -->
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="EntityFramework" publicKeyToken="b77a5c561934e089" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Oracle.ManagedDataAccess" publicKeyToken="89b483f429c47342" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.121.1.0" newVersion="4.121.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

</configuration>