   <applicationSettings>
      <!-- Caravan Configuration Sections (START) -->      
      <Finsa.Caravan.WebForms.Properties.Settings>
         <setting name="AjaxLookupsXmlPath" serializeAs="String">
            <value>~/App_Data/XmlSettings/AjaxLookups</value>
         </setting>
         <setting name="ControlsExtendersFolder" serializeAs="String">
            <value>~/MyFLEX/ControlExtenders</value>
         </setting>
         <setting name="DynamicReportsFolder" serializeAs="String">
            <value>~/App_Data/XmlSettings/DynamicReports</value>
         </setting>
         <setting name="EnableOutputCompression" serializeAs="String">
            <value>False</value>
         </setting>
         <setting name="EnableOutputMinification" serializeAs="String">
            <value>True</value>
         </setting>
         <setting name="EnableSecurity" serializeAs="String">
            <value>False</value>
         </setting>
         <setting name="ErrorManagerTypeInfo" serializeAs="String">
            <value>FLEX.Sample.WebUI.MyFLEX.Managers.ErrorManager, FLEX.Sample.WebUI</value>
         </setting>
         <setting name="LookupsXmlPath" serializeAs="String">
            <value>~/App_Data/XmlSettings/Lookups</value>
         </setting>
         <setting name="MenuBarBrandClick" serializeAs="String">
            <value>window.location.href = 'http://www.finsa.it';</value>
         </setting>
         <setting name="MenuBarHomeClick" serializeAs="String">
            <value>FLEX_RunFromMenu('Home.aspx');</value>
         </setting>
         <setting name="MenuBarInfoClick" serializeAs="String">
            <value>return true;</value>
         </setting>
         <setting name="MenuBarLogoutClick" serializeAs="String">
            <value>return true;</value>
         </setting>
         <setting name="MenuBarXmlPath" serializeAs="String">
            <value>~/App_Data/XmlSettings/MenuBar.xml</value>
         </setting>
         <setting name="PageManagerTypeInfo" serializeAs="String">
            <value>FLEX.Sample.WebUI.MyFLEX.Managers.PageManager, FLEX.Sample.WebUI</value>
         </setting>
         <setting name="SecurityManagerTypeInfo" serializeAs="String">
            <value>FLEX.Sample.WebUI.MyFLEX.Managers.SecurityManager, FLEX.Sample.WebUI</value>
         </setting>
         <setting name="SessionExpiredPageUrl" serializeAs="String">
            <value>Expired.aspx</value>
         </setting>
      </Finsa.Caravan.WebForms.Properties.Settings>
      <!-- Caravan Configuration Sections (END) -->

      <!-- Armando Configuration Section (START) -->
      <PommaLabs.Armando.My.MySettings>
         <setting name="ScriptsLanguage" serializeAs="String">
            <value>C#</value>
         </setting>
         <setting name="ScriptsPath" serializeAs="String">
            <value>~/App_Data/DataScripts</value>
         </setting>
         <setting name="CompiledScriptsPath" serializeAs="String">
            <value>~/App_Data/CompiledScripts</value>
         </setting>
         <setting name="EnableScriptsDebug" serializeAs="String">
            <value>True</value>
         </setting>
      </PommaLabs.Armando.My.MySettings>
      <!-- Armando Configuration Section (END) -->
