@using Finsa.Caravan.Mvc.Layouts

<!DOCTYPE html>

<html>
   <head>
      <!-- Required for IE compatibility -->
      <meta http-equiv="X-UA-Compatible" content="IE=11; IE=10; IE=9; IE=EDGE" />
      <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
      <meta name="viewport" content="width=device-width, initial-scale=1.0" />

      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/fonts.min.css" rel="stylesheet" media="screen" />
      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/jquery-ui.min.css" rel="stylesheet" media="screen" />
      
      <!-- "DoNotCacheTag" is needed to avoid issues with IE9 -->
      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/bootstrap.min.css@(CoreLayoutHelper.DoNotCacheTag)" rel="stylesheet" media="screen" />
      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/bootstrap-datepicker3.min.css" rel="stylesheet" media="screen" />
      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/bootstrap-switch.min.css" rel="stylesheet" media="screen" />
      
      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/flex.min.css" rel="stylesheet" media="screen" />
      
      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/bootstrap-table.min.css" rel="stylesheet" media="screen" />
      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/select2.min.css@(CoreLayoutHelper.DoNotCacheTag)" rel="stylesheet" media="screen" />
      <link href="@(CoreLayoutHelper.CaravanPath)/Styles/select2-bootstrap.min.css" rel="stylesheet" media="screen" />

      <!-- Respond.js and html5shiv must be put AFTER CSS for Bootstrap -->
      <!--[if lt IE 9]>  
         <script src="@(CoreLayoutHelper.CaravanPath)/Scripts/html5shiv.min.js"></script>
         <script src="@(CoreLayoutHelper.CaravanPath)/Scripts/respond.min.js"></script>
      <![endif]-->
      
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/base64.min.js"></script>

      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/jquery-2.1.1.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/underscore.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/backbone.min.js"></script>

      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/jquery-ui.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/jquery.unobtrusive-ajax.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/spin.min.js"></script>

      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/bootstrap.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/bootstrap-hover-dropdown.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/bootstrap-datepicker.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/bootstrap-maxlength.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/bootstrap-switch.min.js"></script>

      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/bootstrap-table.min.js"></script>
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/select2.min.js"></script>
      
      <!-- Custom scripts, use DoNotCacheTag to allow emergency updates! -->
      <script type="text/javascript" src="@(CoreLayoutHelper.CaravanPath)/Scripts/flex.min.js@(CoreLayoutHelper.DoNotCacheTag)"></script>

      <script type="text/javascript" src="@(CoreLayoutHelper.MyCaravanPath)/Scripts/autocompletes.min.js@(CoreLayoutHelper.DoNotCacheTag)"></script>
      
      <script type="text/javascript">
         // JS settings
         settings.rootPath = "@CoreLayoutHelper.RootPath";
         settings.flexPath = "@CoreLayoutHelper.CaravanPath";
         settings.myFlexPath = "@CoreLayoutHelper.MyCaravanPath";
      </script>
      
      @RenderSection("HeadSection")
   </head>
   
   <body>
      @RenderSection("BodySection")
   </body>

</html>